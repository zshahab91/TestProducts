{
    "presets": ["next/babel"],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "@components": "./components",
            "@services": "./services",
            "@styles": "./styles",
            "@utils": "./utils"
          },
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  }
  